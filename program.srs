fn fib( n: i32 ) {
    res = 1;
    s = 0;
    if ( n > 2 ) {
        res = fib( n - 1 );
        s = fib( n - 2 );
        0
    };
    res + s
}

fn print_fibs_up_to_n( n: i32 ) {
    if n > 1 {
        print_fibs_up_to_n( n - 1 );
        0
    };
    print( n, ": ", fib( n ) );
    0
}

fn fact(n: i32) {
    if n <= 1 {
        res = 1;
        res
    } else {
        res = fact( n - 1 );
        n * res
    }
}

fn main( ) {
    print_fibs_up_to_n( 15 );
    print( fact( 7 ) );
    0
}

